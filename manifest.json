{
    "name": "Picasa Image Uploader",
    "version": "0.2",
    "description": "Save images from web directly to your picasa web albums.",
    //"default_locale": "en",
    "browser_action": {
        "default_icon": "img/icon.png"
    },
    // icon's author http://saviourmachine.deviantart.com/
    "icons" : {
        "16": "img/icon.png",
        "32": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png",
        "256": "img/icon.png"
    },
    "background_page": "html/background.html",
    "permissions": [
        "contextMenus",
        "http://*/",
        "https://*/"
    ]
}
